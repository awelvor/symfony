     <td>{{ actif.c10}}</td>
     <td>{{ actif.c11}}</td>
     <td>{{ actif.c13}}</td>
     <td>{{ actif.c14}}</td>
     <td>{{ actif.total}}</td>
     </tr>
     {% endfor %}
    </table>

   

