    <td><button type="button" class="btn btn-link">Total</button></td>
    </tr>
     {% for actif in actifs %}
     <tr>
     <td>{{ actif.date }}</td>
     <td>{{ actif.c1 }}</td>
     <td>{{ actif.sg_pea_ni }}</td>
     <td>{{ actif.sg_sequoia_ni }}</td>
     <td>{{ actif.c6}}</td>
